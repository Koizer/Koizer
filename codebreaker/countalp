#include<bits/stdc++.h>
using namespace std;

int main(){
	
	 int k;
	 string l;
	 cin>>k>>l;
	 
	 map<char, int>mp;
	 
	 for(int i=97;i<=122;i++){
		mp[char(i)]=0;
		}
	 
	 
	 for(char i:l){
		mp[i]++;
		}
	for(auto it=mp.begin();it!=mp.end();it++){
		cout<<it->first<<' '<<it->second<<'\n';
	}
	


}

